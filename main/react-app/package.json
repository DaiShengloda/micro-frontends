    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^12.8.3",
    "antd": "^3.26.9",
    "classnames": "^2.3.1",
    "customize-cra": "^1.0.0",
    "less": "^4.1.1",
    "less-loader": "^5.0.0",
    "lodash": "^4.17.21",
    "mobx": "^5.15.1",
    "mobx-react": "^6.1.4",
    "qiankun": "^2.4.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "web-vitals": "^1.1.1"
  },
